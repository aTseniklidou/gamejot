{
  "cli": {
    "version": ">= 0.42.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "production": {}
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./service-key.json"
      }
    },
    "testing": {
      "android": {
        "serviceAccountKeyPath": "./service-key.json",
        "releaseStatus": "draft"
      }
    }
  }
}
